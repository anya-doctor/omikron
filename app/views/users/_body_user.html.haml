%table.table.table-hover
  %thead
    %tr
      %th.column-20 email
      %th.column-20 Edit
      %th.column-25 Delete
  %tbody  
    - @users.each do |user|
      %tr
        %td
          = link_to user.email, user_path(user)
        %td
          = link_to "Edit", edit_user_path(user)
        %td
          = link_to "Delete", "#"